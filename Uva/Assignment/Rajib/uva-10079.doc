                        /// Uva - Pizza Cutting

#include<stdio.h>
int main()
{
    int p,i,nm;
    long int q,r;
    while(scanf("%d",&nm)!=EOF&&nm>=0)
    {
        p=0;
        q=1;
        for(i=0;i<=nm;i++)
        {   r=p+q;
            p=p+1;
            q=r;
        }
        printf("%ld\n",r);
    }
    return 0;
}
